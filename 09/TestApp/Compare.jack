class Compare{
	function boolean compare(String word1, String word2){
		var int i;
		let i = 0;
		
		if(word1.length() = word2.length()){
			while(i < word1.length()){
			
			if(~(word1.charAt(i) = word2.charAt(i))){
				return false;
			}
			let i = i + 1;
			}
			return true;
		}
		
		
		return false;
	}
	


}