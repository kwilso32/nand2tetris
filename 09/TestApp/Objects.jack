class Objects{
  field String name;
  field Array objects;
  field int count;
  constructor Objects new(String objName, int length){
    let name = objName;
    let count = 0;
    let objects = Array.new(length);
  }

  method getName(){
    return name;
  }
  method Array getObjects(){
    return objects;
  }
  method void addObjects(String object){
    objects[count] = object;
    let count = count + 1;
  }

}
