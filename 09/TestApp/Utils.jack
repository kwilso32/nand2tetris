class Utils {

    function int mod(int a, int b) {
        var int d;
        var int r;
        let d = Math.divide(a,b);
        let r = a - (b * d);
        return r;
    }

    function int increment(int a){
      return (a + 1);
    }
    function int decrement(int a){
      return (a - 1);
    }
}
