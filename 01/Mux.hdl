/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = sel, out = x);
    Nand(a = a, b = x, out = y);
    Nand(a = sel, b = b, out = z);
    Xor(a = y, b = z, out = out);
}
